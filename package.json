  "dependencies": {
    "cosmiconfig": "4.0.0",
    "create-jest-runner": "^0.3.0",
    "lodash.kebabcase": "^4.1.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-plugin-filename": "^0.0.1",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4"
  },
  "jest": {
    "projects": [
      "jest-shellcheck-linter.config.js",
      "jest-test.config.js"
    ]
